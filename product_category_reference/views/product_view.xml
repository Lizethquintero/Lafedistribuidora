        <record id="product_category_form_reference_cat_view" model="ir.ui.view">
            <field name="name">product.category.reference.cat.form</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='parent_id']" position="after">
                    <field name="sequence_id" require="False"/>
		    <field name="sequence_prefix"/>
                </xpath>
            </field>
        </record>
        
        <record id="stock_quant_inherit_tree_view" model="ir.ui.view">
            <field name="name">stock.quant.inherit.tree.view</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_uom_id']" position="before">
                    <field name="lot_id"/>
                    <field name="in_date"/>
                    <field name="removal_date"/>
                </xpath>
                <xpath expr="//field[@name='value']" position="before">
                    <field name="comercial_value" widget="monetary"/>
                </xpath>
            </field>
        </record>
        
        
        
    </data>

<odoo>
    <record id="stock_picking_form_invima" model="ir.ui.view">
        <field name="name">stock.picking.form.invima</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//form/sheet/notebook/page[3]/field[1]/tree/field[@name='product_id']" position="after">
                  <field name="invima_code"/>
             </xpath>
        </field>
    </record>
</odoo>
